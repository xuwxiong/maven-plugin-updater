<?xml version="1.0" encoding="UTF-8"?><project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
    <modelVersion>4.0.0</modelVersion>
    
    <parent>
        <groupId>intact.devtools</groupId>
        <artifactId>intact-parent-pom</artifactId>
        <version>1.1.0</version>
    </parent>
    
    <groupId>intact.soa-external.dmti</groupId>
    <artifactId>elevation-service-consumer</artifactId>
    <packaging>jar</packaging>
    <version>5.13.3.0.0.0.D11PRODFIX-SNAPSHOT</version>
    <name>${project.artifactId}</name>
    <description>Consumer module for eleva web service (HPSM)</description>
    
    <properties>
        <shortGroupId>soa.it.elevation-service-consumer</shortGroupId>
        <schemaDirectory>src/main/resources/com/intact/soa/external/dmti/elevationserviceconsumer/elevationservice/</schemaDirectory>
        <scmRepositoryWorkspace>DEV_${user.name}_${shortGroupId}.${project.artifactId}_MASTER_WS</scmRepositoryWorkspace>
         <branch>MASTER</branch>
   </properties>
    
    <scm>
        <connection>scm:jazz:https://prod-rtc-b2e.iad.ca.inet/ccm:${scmRepositoryWorkspace}</connection>
    </scm>
	
	<distributionManagement>
		<site>
			<id>intact-site</id>
			<name>intact site repository</name>
			<url>dav:${project.url}</url>
		</site>
	</distributionManagement>
    
    <dependencies>
        <dependency>
            <groupId>intact.soa</groupId>
            <artifactId>commons-consumer</artifactId>
            <version>2.0.0-SNAPSHOT</version>
        </dependency>
        <dependency>
			<groupId>log4j</groupId>
			<artifactId>log4j</artifactId>
			<version>1.2.17</version>
		</dependency>
		
        <dependency>
            <groupId>intact.commons</groupId>
            <artifactId>bean-mapping</artifactId>
            <version>1.1.3</version>
        </dependency>
        
        <dependency>
            <groupId>intact.soa</groupId>
            <artifactId>ws-commons-message</artifactId>
            <version>2.0.3</version>
        </dependency>
        
        <dependency>
			<groupId>intact.soa-external.dmti</groupId>
			<artifactId>authentication-service-consumer</artifactId>
			<version>1.0.0-SNAPSHOT</version>
		</dependency>
        
      <dependency>
			<groupId>intact.business-object-domain</groupId>
			<artifactId>basic-data-types</artifactId>
			<version>2.0.0-SNAPSHOT</version>
		</dependency>
        <dependency>
            <groupId>intact.commons</groupId>
            <artifactId>commons-utils</artifactId>
            <version>1.0.5</version>
        </dependency>
        
        
        <dependency>
            <groupId>intact.commons</groupId>
            <artifactId>dependencies-spring</artifactId>
            <version>3.1.4-RELEASE-3</version>
            <type>pom</type>
            <scope>provided</scope>
        </dependency>
        
        <dependency>
            <groupId>intact.commons</groupId>
            <artifactId>unit-tests-util</artifactId>
            <version>1.0.5</version>
            <scope>test</scope>
        </dependency>
        <dependency>
            <groupId>junit</groupId>
            <artifactId>junit</artifactId>
            <version>4.11</version>
            <scope>test</scope>
        </dependency>
    </dependencies>
    
    <build>
        <pluginManagement>
            <plugins>
                <!--This plugin's configuration is used to store Eclipse m2e settings only. It has no influence on the Maven build itself.-->
                <plugin>
                    <groupId>org.eclipse.m2e</groupId>
                    <artifactId>lifecycle-mapping</artifactId>
                    <version>1.0.0</version>
                    <configuration>
                        <lifecycleMappingMetadata>
                            <pluginExecutions>
                                <pluginExecution>
                                    <pluginExecutionFilter>
                                        <groupId>
                                            org.apache.maven.plugins
                                        </groupId>
                                        <artifactId>
                                            maven-antrun-plugin
                                        </artifactId>
                                        <versionRange>
                                            [1.1,)
                                        </versionRange>
                                        <goals>
                                            <goal>run</goal>
                                        </goals>
                                    </pluginExecutionFilter>
                                    <action>
                                        <ignore/>
                                    </action>
                                </pluginExecution>
                                <pluginExecution>
                                    <pluginExecutionFilter>
                                        <groupId>
                                            org.apache.maven.plugins
                                        </groupId>
                                        <artifactId>
                                            maven-dependency-plugin
                                        </artifactId>
                                        <versionRange>
                                            [2.7,)
                                        </versionRange>
                                        <goals>
                                            <goal>
                                                unpack-dependencies
                                            </goal>
                                        </goals>
                                    </pluginExecutionFilter>
                                    <action>
                                        <ignore/>
                                    </action>
                                </pluginExecution>
                                <pluginExecution>
                                    <pluginExecutionFilter>
                                        <groupId>
                                            org.jvnet.jaxb2.maven2
                                        </groupId>
                                        <artifactId>
                                            maven-jaxb2-plugin
                                        </artifactId>
                                        <versionRange>
                                            [0.7.3,)
                                        </versionRange>
                                        <goals>
                                            <goal>xjc</goal>
                                            <goal>generate</goal>
                                        </goals>
                                    </pluginExecutionFilter>
									<action>
                                        <ignore/>
                                    </action>
                                </pluginExecution>
                            </pluginExecutions>
                        </lifecycleMappingMetadata>
                    </configuration>
                </plugin>
            </plugins>
        </pluginManagement>

        <!-- load resources : mvn initialize-->
        <!-- the resources that will be used to compile the src: java classes, properties,...-->
        <!-- do not include unpacked dependencies in the jar -->
        <resources>
            <resource>
                <directory>src/main/resources</directory>
                <excludes>
                    <exclude>wsdl/**</exclude>
                    <exclude>xsd/**</exclude>
                </excludes>
                <filtering>false</filtering>
            </resource>
        </resources>
        
        <plugins>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-surefire-plugin</artifactId>
                <configuration>
                    <excludes>
                        <exclude>**/TestUtils.java</exclude>
                    </excludes>
                </configuration>
            </plugin>            
            <plugin>
                <groupId>org.jvnet.jaxb2.maven2</groupId>
                <artifactId>maven-jaxb2-plugin</artifactId>
                <version>0.7.3</version>
                <executions>
                    <execution>
                        <goals>
                            <goal>generate</goal>
                        </goals>
                    </execution>
                </executions>
                <configuration>
                    <extension>true</extension>
                    <debug>true</debug>
                    <verbose>true</verbose>
                    <episode>true</episode>
                    <schemaDirectory>${schemaDirectory}</schemaDirectory>
                    <schemaIncludes>
                        <include>wsdl/ElevationService.wsdl</include>
                        <include>xsd/w3c-adapters.xsd</include> 
                    </schemaIncludes>
                    <bindingDirectory>${schemaDirectory}/xsd/jaxb2</bindingDirectory>
                    <bindingIncludes>
                        <include>bindings.xjb</include>
                    </bindingIncludes>
                    <forceRegenerate>true</forceRegenerate>
                    <args>
                        <arg>-Xboolean-getter</arg>
                    </args>
                    <plugins>
                        <plugin>
                            <groupId>com.nebulent.xjc</groupId>
                            <artifactId>boolean-getter</artifactId>
                            <version>0.5</version>
                        </plugin>
                    </plugins>
                </configuration>
            </plugin>
            <plugin>
                <artifactId>maven-antrun-plugin</artifactId>
                <executions>
                    <execution>
                        <phase>generate-sources</phase>
                        <goals>
                            <goal>run</goal>
                        </goals>                        
                    </execution>
                </executions>
            </plugin>
			 <plugin>
			    <groupId>org.codehaus.mojo</groupId>
			    <artifactId>build-helper-maven-plugin</artifactId>
			    <executions>
			        <execution>
			            <id>add-source</id>
			            <phase>generate-sources</phase>
			            <goals>
			                <goal>add-source</goal>
			            </goals>
			            <configuration>
			                <sources>
			                    <source>${project.build.directory}/generated-sources/xjc/</source>
			                </sources>
			            </configuration>
			        </execution>
			    </executions>
			 </plugin>
        </plugins>
    </build>
</project>